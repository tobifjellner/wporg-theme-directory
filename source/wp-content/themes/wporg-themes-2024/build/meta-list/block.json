{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wporg/meta-list",
  "version": "0.1.0",
  "title": "Meta List",
  "category": "design",
  "icon": "",
  "description": "Display a theme's metadata as a list.",
  "textdomain": "wporg",
  "attributes": {
    "showLabel": {
      "default": true,
      "type": "boolean"
    },
    "meta": {
      "type": "array"
    }
  },
  "supports": {
    "html": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    }
  },
  "usesContext": [
    "postId"
  ],
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}